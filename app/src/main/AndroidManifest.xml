<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.joonhee_pak.stricfilm">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/newicon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.joonhee_pak.stricfilm.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.joonhee_pak.stricfilm.DetailsActivity"
            android:theme="@style/Transparent" />
        <activity
            android:name="com.joonhee_pak.stricfilm.FavoriteList" />
        <activity
            android:name="com.joonhee_pak.stricfilm.ReviewsList" />
    </application>

</manifest>